package chapter02;

public class PromotionExample {
	public static void main(String[] args) {
		byte byteValue=10;
		int intValue=byteValue;
		System.out.println("intValue: " + intValue);
		
		char charValue='°¡';
		intValue = charValue;		
		System.out.println("intValue: " + intValue);
		
		intValue = 50;
		long longValue=intValue;
		System.out.println("intValue: " + intValue);
	}
}
